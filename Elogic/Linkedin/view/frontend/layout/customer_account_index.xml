<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="customer.account.dashboard.info.blocks">
            <block class="Magento\Framework\View\Element\Template"
                   name="customer_account_linkedin_info" template="Elogic_Linkedin::linkedin_profile_info.phtml"
                   cacheable="false">
                <arguments>
                    <argument name="linkedin_attribute_data" xsi:type="helper"
                    helper="Elogic\Linkedin\Block\DataProviders\LinkedinAttributeData::getLinkedinAttributeData"/>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>
